Proceso Ejercicio1
	Definir hora,min,sec,total como Entero;
	Escribir Sin Saltar"Ingrese valor de hora: ";
	Leer hora;
	Escribir Sin Saltar"Ingrese valor de minutos: ";
	leer min;
	Escribir Sin Saltar"Ingrese valor de segundos: ";
	leer sec;
	total<-hora*3600+min*60+sec;
	Escribir total;
	
FinProceso
